---
  import ContentBar from './ContentBar.svelte';
  import SettingsBar from './SettingsBar.astro';
  const { titles, menus } = Astro.props;
---

  <aside id="sidebar">
    <nav role="navigation" id="content-nav" aria-hidden="true" aria-labelledby="menu-button">
      <ContentBar titles={titles} menus={menus} />
      <SettingsBar />
    </nav>
  </aside>